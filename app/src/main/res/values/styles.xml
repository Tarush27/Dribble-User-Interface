<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="DribbleToolbar" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="MyMaterialToolbar.TitleText" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">25sp</item>
        <item name="android:fontFamily">sans-serif-black</item>
    </style>

    <style name="MyOutlineButton" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="strokeColor">#e6e6e6</item>
        <item name="fontFamily">sans-serif</item>
        <item name="cornerRadius">8dp</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="InstaUserDescriptionOne" parent="Widget.MaterialComponents.TextView">
        <item name="android:text">
            Calling ALL SPEAKERS who want to get paid to Speak!
        </item>
        <item name="android:layout_marginLeft">25dp</item>
        <item name="android:layout_marginTop">18dp</item>
        <item name="fontFamily">sans-serif-black</item>


    </style>

    <style name="InstaUserDescriptionTwo" parent="Widget.MaterialComponents.TextView">
        <item name="android:text">
            (JOIN MY CLUB: Paid to Speak-Public Speaking Toolbox
        </item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">25dp</item>
        <item name="fontFamily">sans-serif-black</item>
    </style>

    <style name="PostedAudioTextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">#000000</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:paddingRight">30dp</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="PlaylistsTextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">#95A5A6</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingLeft">50dp</item>
        <item name="android:paddingRight">50dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MusicNameTextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">#000000</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="FollowMeOnTextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">#000000</item>
        <item name="fontFamily">sans-serif</item>
    </style>


</resources>